<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.sdd_project" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/darkblue2</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >@color/darkblue2</item>
    </style>
    <style name="Theme.sdd_project.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.sdd_project.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.sdd_project.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>